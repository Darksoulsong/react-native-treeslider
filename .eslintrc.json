{
  "extends": [
    "airbnb",
    "prettier",
    "plugin:flowtype/recommended",
    "plugin:prettier/recommended",
    "prettier/react"
  ],
  "plugins": ["jsx-a11y", "prettier", "flowtype"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/destructuring-assignment": "off",
    "react/sort-comp": "off",
    "react/jsx-filename-extension": "off"
  }
}
